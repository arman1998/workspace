package bucky;
public class MultipleMethods {
     public static void main(String[] args){
    	 int array[] = {1,3,6,4,8,9};
    	 arrayChange(array);
    	 for(int i: array){
    		 System.out.println(i);
    	 }
     }
     public static void arrayChange(int x[]){
    	 for(int i=0;i<x.length;i++){
    		 x[i]+=5;
    	 }
     }
}
