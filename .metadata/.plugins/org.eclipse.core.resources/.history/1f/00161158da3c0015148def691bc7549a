package Bucky;
import java.util.Scanner;
public class A {
     public static void main(String args[]){
    	 Scanner bucky = new Scanner(System.in);
    	 double a,b,c;
    	 System.out.println("Enter a: ");
    	 a = bucky.nextDouble();
    	 System.out.println("Enter b: ");
    	 b = bucky.nextDouble();
    	 c = a+b;
    	 System.out.println(c);
     }
}
